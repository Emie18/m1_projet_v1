<table>
  <thead>
    <tr>
      <th>Nom Prénom</th>
     
      <th>Date début-fin</th>
      <th>Titre stage</th>
      <th>Tuteur académique</th>
      <th>Soutenance</th>
      <th>Rapport</th>
      <th>Évaluation entreprise</th>
    </tr>
  </thead>
  <tbody id="tableBody">
{% for stage in stages %}
    <tr onclick="afficherDetails({{ stage.id }});" class = "lignetableau">
      <td>{{ stage.apprenant.nom }} {{ stage.apprenant.prenom }}</td>
      <td>{{ stage.dateDebut|date('d-m-Y') }} - {{ stage.dateFin|date('d-m-Y') }}</td>
      <td>{{stage.titre}}</td>
      <td>{{stage.tuteurIsen.nom}} {{stage.tuteurIsen.prenom}}</td>
      <td>{{stage.soutenance}}</td>
      <td>{{stage.rapport}}</td>
      <td>{{stage.evalEntreprise}}</td>
    </tr>
    {% endfor %}
  </tbody>
</table>
